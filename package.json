{
  "name": "kennelog-server",
  "description": "Kennelog server",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "roris",
    "email": "roris@users.noreply.github.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "check-types": "tsc --noEmit",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint */**/*.{js,ts}",
    "migrate-latest": "knex migrate:latest",
    "migrate-up": "knex migrate:up",
    "migrate-down": "knex migrate:down",
    "migrate-rollback": "knex migrate:rollback",
    "make-migrate": "knex migrate:make -x ts",
    "start": "yarn run build && node dist/",
    "test": "yarn run build && jest --coverage --coverageReporters=text-lcov --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.3.0-pre.1",
    "@feathersjs/authentication-local": "^4.3.0-pre.1",
    "@feathersjs/authentication-oauth": "^4.3.0-pre.1",
    "@feathersjs/configuration": "^4.3.0-pre.1",
    "@feathersjs/errors": "^4.3.0-pre.1",
    "@feathersjs/express": "^4.3.0-pre.1",
    "@feathersjs/feathers": "^4.3.0-pre.1",
    "@feathersjs/socketio": "^4.3.0-pre.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-blob": "^2.1.0",
    "feathers-objection": "^4.1.1",
    "fs-blob-store": "^5.2.1",
    "helmet": "^3.20.0",
    "knex": "^0.19.1",
    "objection": "^1.6.9",
    "pg": "^7.12.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/compression": "^1.0.0",
    "@types/cors": "^2.8.5",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.16",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/serve-favicon": "^2.2.30",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "axios": "^0.19.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "shx": "^0.3.2",
    "sqlite3": "^4.0.9",
    "ts-jest": "^24.0.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --bail"
    }
  }
}
